{
  "name": "cpc-playground",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:test": "vite --mode test",
    "dev:prod": "vite --mode production",
    "build": "tsc -b && vite build",
    "build:test": "tsc -b && vite build --mode test",
    "build:prod": "tsc -b && vite build --mode production",
    "lint": "biome check --write .",
    "format": "biome format --write .",
    "check": "biome check . && node scripts/check-barrel-exports.js",
    "check:barrels": "node scripts/check-barrel-exports.js",
    "check:console": "node scripts/check-console-usage.js",
    "typecheck": "tsc --noEmit",
    "typecheck:comprehensive": "node scripts/typecheck-comprehensive.js",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:integration": "TEST_INTEGRATION=true vitest run --testNamePattern='Integration'",
    "test:ui": "vitest --ui",
    "prepare": "husky"
  },
  "dependencies": {
    "@netlify/blobs": "^10.5.0",
    "@netlify/functions": "^5.1.1",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/supabase-js": "^2.89.0",
    "ansi-to-react": "^6.1.6",
    "clsx": "^2.1.1",
    "jotai": "^2.16.0",
    "nanoid": "^5.1.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.16"
  },
  "packageManager": "pnpm@10.11.0"
}
